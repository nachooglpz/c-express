{
  "name": "c-express-benchmarks",
  "version": "1.0.0",
  "description": "Performance benchmarks comparing C-Express vs Express.js",
  "scripts": {
    "bench": "node benchmark-runner.js",
    "bench:quick": "node benchmark-runner.js --quick",
    "bench:json": "node json-benchmark.js",
    "bench:routing": "node routing-benchmark.js",
    "bench:middleware": "node middleware-benchmark.js",
    "bench:all": "npm run bench:json && npm run bench:routing && npm run bench:middleware",
    "install-deps": "npm install express autocannon clinic"
  },
  "dependencies": {
    "express": "^4.18.2",
    "autocannon": "^7.12.0",
    "clinic": "^13.0.0",
    "table": "^6.8.1",
    "chalk": "^4.1.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}